<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>

<GridPane fx:id="grid" stylesheets="@../../application.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.presentation.logic.CheckpointListController">
  <columnConstraints>
    <ColumnConstraints />
      <ColumnConstraints hgrow="SOMETIMES" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints vgrow="NEVER" />
  </rowConstraints>
   <children>
      <VBox fx:id="environmentLabelBox" onMouseClicked="#environmentLabelClicked" prefWidth="200" minWidth="200" maxWidth="200">
         <children>
            <Label fx:id="nameLabel" />
            <Label fx:id="isSQLLabel" />
         </children>
         <styleClass>
         	<String fx:value="checkpoint_environment_label_holder" />
            <String fx:value="center_label_holder" />
         </styleClass>
      </VBox>
   </children>
</GridPane>
