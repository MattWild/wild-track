            <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text=" If entering more than one collector, separate each collector IP with a comma." wrapText="true" GridPane.columnIndex="1" GridPane.columnSpan="5" GridPane.rowIndex="2" />
