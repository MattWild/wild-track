            <Button mnemonicParsing="false" onMouseClicked="#addCollectors" text="Add Meter(s)" GridPane.columnIndex="2" GridPane.rowIndex="6" />
            <Button mnemonicParsing="false" onMouseClicked="#cancel" text="Cancel" GridPane.columnIndex="4" GridPane.rowIndex="6" />
            <TextArea fx:id="collectorIPArea" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.columnSpan="5" GridPane.rowIndex="4" />
