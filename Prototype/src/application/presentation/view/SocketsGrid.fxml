<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.presentation.logic.SocketsGridController">
   <children>
   	<GridPane fx:id="socketsGrid" alignment="TOP_CENTER" hgap="10.0" stylesheets="@../../application.css" vgap="10.0">
   	   <columnConstraints>
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="20.0" />
   	     <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="40.0" />
   	     <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="80.0" />
   	       <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="100.0" />
   	       <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="79.0" />
   	   </columnConstraints>
   	   <rowConstraints>
   	     <RowConstraints minHeight="-Infinity" prefHeight="80.0" vgrow="SOMETIMES" />
   	   </rowConstraints>
   	    <opaqueInsets>
   	       <Insets />
   	    </opaqueInsets>
   	    <children>
   	       <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0" GridPane.columnIndex="1" GridPane.halignment="LEFT">
   	          <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" text="ID" />
                  <ComboBox fx:id="idFilter" prefWidth="150.0" />
   	          </children>
   	       </VBox>
   	       <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0" GridPane.columnIndex="2">
   	          <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" text="Socket Form" />
   	             <ComboBox fx:id="formFilter" prefWidth="150.0" />
   	          </children>
   	       </VBox>
   	       <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0" GridPane.columnIndex="3">
   	          <children>
   	             <Label alignment="CENTER" contentDisplay="CENTER" text="Load/No Load" />
                  <ComboBox fx:id="nloadFilter" prefWidth="150.0" />
   	          </children>
   	       </VBox>
   	       <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0" GridPane.columnIndex="4">
   	          <children>
   	             <Label alignment="CENTER" contentDisplay="CENTER" text="Location" />
   	             <ComboBox fx:id="locationFilter" prefWidth="150.0" />
   	          </children>
   	       </VBox>
   	    </children>
   	 </GridPane>
   </children>
</AnchorPane>
